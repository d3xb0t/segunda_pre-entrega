{{#if isValid}}
<div>
    {{#each docs}}
    <div>
        <p>Titulo: {{this.title}} &nbsp; Description: {{this.description}}</p>
        <p>Category: {{this.category}} &nbsp; Status: {{this.status}}</p>
        <p>id: {{this.id}}</p>
    </div>
    <button  onclick="imprimir('{{this.id}}')">Agregar</button> 
    <br/>
    {{/each}}
</div>
<br/>

<div>
    {{#if hasPrevPage}}
        <a href={{this.prevLink}}><< Anterior</a>
    {{/if}}
    &nbsp;
    {{this.page}}
    &nbsp;
     {{#if hasNextPage}}
        <a href={{this.nextLink}}>Siguiente >></a>
    {{/if}} 
</div>

{{else}}
<h1>No hay peliculas para mostrar</h1>
{{/if}}


<script src="js/catalogue.js"></script>